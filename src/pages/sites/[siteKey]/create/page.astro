---
import { CreatePageForm } from '@client/sites/PageApp/CreatePageForm';
import PageLayout from '@layouts/Page.astro';

const { siteKey } = Astro.params;
const site = `${siteKey}`;

const name = Astro.url.searchParams.get('name') || '';
---
<PageLayout>
  <CreatePageForm siteKey={site} name={name} client:only="solid-js"/>
</PageLayout>
